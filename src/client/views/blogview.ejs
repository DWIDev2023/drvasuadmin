    <div class="container">
        <div class="blogMain space40"> 
            <div class="blogSection pt-0">
                <div class="blogLeft">
                    <div class="blogDetails">
                        <h5>TOPIC OF BLOG</h5>
                        <ul>
                            <li>
                                <img src="../assets/images/calenderIcon.svg" alt="">
                              
<% let a = bloginfo[0].createdAt + "" %>
                        <span><%= a.slice(4,15) %></span>

                            </li>
                            <li>
                                <a href="javascript:void(0)">
                                    <img src="../assets/images/userIcon.svg" alt="">
                                    <span> <%= doctorinfo[0].firstName %> <%= doctorinfo[0].lastName %></span>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:void(0)">
                                    <img src="../assets/images/commentIcon.svg" alt="">
                                    <span> <%= comments.length %> comment</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="blogData">
                        <img src="../uploads/<%= bloginfo[0].displayimg %>" alt="">
              
                        <p id="blogdesc" ><%=  blogdesc %> </p>

                    </div>
                    <div class="postextralinks">
                        <div class="sharePost">
                            <h5>SHARE THE POST</h5>
                            <ul>
                                <li><a href=""><img src="../assets/images/blog-facebook.svg" alt=""></a></li>
                                <li><a href=""><img src="../assets/images/blog-instagram.svg" alt=""></a></li>
                                <li><a href=""><img src="../assets/images/blog-linkedin.svg" alt=""></a></li>
                                <li><a href=""><img src="../assets/images/blog-twitter.svg" alt=""></a></li>
                            </ul>
                        </div>
                        <div class="aboutAuthor">
                            <h5>ABOUT AUTHOR</h5>
                            <div class="authorsInfo">
                                <span><img src="/uploads/<%= doctorinfo[0].imgsrc %>" alt=""></span>
                                <div>
                                    <h6><%= doctorinfo[0].firstName %> <%= doctorinfo[0].lastName %></h6>
                                    <p><%=   doctorinfo[0].doctorabout   %>           </p>
                                </div>
                            </div>
                        </div>
<div>
<h5> comments </h5>
<% for(let i=0; i < comments.length; i++){ %>
<ul style="margin-bottom: 16px">
<li>name : <%= comments[i].name %></li>
<li>comment : <%= comments[i].comment %></li>
</ul>
<% } %>
</div>
                        <form class="postForm" action="/comment?blogid=<%= bloginfo[0]._id %>" method="post">
                            <h5>Submit a Comment</h5>
                            <ul>
                                <li>
                                    <h6>Name:</h6>
                                    <input placeholder="Name" type="text" name="name">
                                </li>
                                <li>
                                    <h6>Your Email Address:</h6>
                                    <input placeholder="Your Email Address" type="text" name="email">
                                </li>
                                <li>
                                    <h6>Comment:</h6>
                                    <textarea cols="10" rows="6" placeholder="Comment" name="comment"></textarea>
                                </li>
                            </ul>
                            <button class="button">Submit</button>
                        </form>
                    </div>
                </div>
                <div class="blogRight">
                    <form class="postContactForm" action="/submitcontact" method="post">
                        <h4>Contact</h4>
                        <ul>
                            <li>
                                <input type="text" name="firstname" placeholder="Name">
                            </li>
                            <li>
                                <input type="text" name="phonenumber" placeholder="Phone">
                            </li>
                            <li>
                                <input id="dateTimeCalendar" type="text" placeholder="Date and Time" name="DateTime">
                            </li>
                        </ul>
                        <button class="button">Submit</button>
                    </form>
                    <div class="latestPost">
                        <h4>Latest Blogs</h4>
                        <ul>
 <% for(let i=0; i< blogs.length; i++) { %>
                    <% if(blogs[i].type == 0){%>

                            <li>
                                <a href="/viewblog?blogid=<%=blogs[i]._id %>&page=1">
                                    <img src="../uploads/<%= blogs[i].displayimg %>" alt="">
                                    <span class="latestBlogContent">
                                        <span class="title"><%= blogs[i].displayname %></span>
<% let a = blogs[i].createdAt + "" %>

                                        <small><img src="../assets/images/calenderIcon.svg" alt=""><%= a.slice(4,15) %></small>
                                    </span>
                                </a>
                            </li>
                <% } }%>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

