        <script>
            var decodeHTML = function (html) {
        var txt = document.createElement('textarea');
        txt.innerHTML = html;
        return txt.value;
    };
    
    let bloghtml =  $('#blogdesc').html()
    var decoded = decodeHTML(bloghtml);
    
    var $geek = $( "#blogdesc" ),
              html = jQuery.parseHTML( decoded ),
              nodeNames = [];
            $geek.html( html );
    // $('#blogdesc').after(bloghtml)
    
    
        </script>
